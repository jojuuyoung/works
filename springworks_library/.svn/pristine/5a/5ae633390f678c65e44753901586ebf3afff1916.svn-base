    <script>
        $(document).ready(function(){
            $('.tbl01 tbody tr').on('mouseenter focusin', function(){
                var posTop = $(this).position().top;

                $(this).addClass('active').siblings().removeClass('active');
                $('.tbl-wrap .active-box').css('top',posTop).stop().animate({'opacity':'1'},300);
            });
            $('.tbl-wrap').on('mouseleave', function(){
                $('.tbl01 tbody tr').removeClass('active');
                $('.tbl-wrap .active-box').css('top','0').stop().animate({'opacity':'0'},0);
            });
        });
    </script>
