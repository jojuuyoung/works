    </div>

    <script>
        $(document).ready(function(){
            var $triger = $('.rule-box'),
                $triger_con = $('.rule-con');

            $triger.bind('click keyup', function(){

                if(!$(this).hasClass('active')){
                    $(this).addClass('active').find($triger_con).stop().slideDown();
                    $(this).siblings().removeClass('active').find($triger_con).stop().slideUp();
                } else if($(this).hasClass('active') && $(this).find('.rule-con').css('display') == 'block'){
                    $(this).removeClass('active').find('.rule-con').stop().slideUp();
                }
            });
        });
    </script>
